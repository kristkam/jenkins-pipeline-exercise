pipeline {
    agent any
    
    stages {
        stage('greetings') {
            steps{
                echo 'Holla Mundos!'
            }
        }
	stage('preparation') {
		steps{
			echo 'hello preparation'
		}
	}
	stage('build') {
		steps{
			echo 'hello build'
		}
	}
	stage('results'){
		steps{
			echo 'hello results'
		}
	}

    }
}

